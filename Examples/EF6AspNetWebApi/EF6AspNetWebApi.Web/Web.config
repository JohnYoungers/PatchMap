  <connectionStrings>
    <add name="LocalEF6" providerName="System.Data.SqlClient" connectionString="Server=(localdb)\mssqllocaldb;Database=EF6;Trusted_Connection=True"/>
  </connectionStrings>
  <system.web>
    <compilation debug="true" targetFramework="4.7">
      <buildProviders>
        <add extension=".html" type="System.Web.Compilation.PageBuildProvider"/>
      </buildProviders>
    </compilation>
    <httpRuntime targetFramework="4.7"/>
  </system.web>
  <location path="wwwroot">
    <!-- Don't cache anything in this folder (i.e. app html files) -->
    <system.webServer>
      <validation validateIntegratedModeConfiguration="false"/>
      <staticContent>
        <clientCache cacheControlMode="DisableCache"/>
        <remove fileExtension=".woff"/>
        <mimeMap fileExtension=".woff" mimeType="application/x-font-woff"/>
        <remove fileExtension=".woff2"/>
        <mimeMap fileExtension=".woff2" mimeType="application/font-woff2"/>
        <remove fileExtension=".less"/>
        <mimeMap fileExtension=".less" mimeType="text/css"/>
      </staticContent>
    </system.webServer>
  </location>
  <system.webServer>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0"/>
      <remove name="OPTIONSVerbHandler"/>
      <remove name="TRACEVerbHandler"/>
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0"/>
    </handlers>
  </system.webServer>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="mssqllocaldb"/>
      </parameters>
    </defaultConnectionFactory>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer"/>
    </providers>
  </entityFramework>
